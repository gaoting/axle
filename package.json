{
  "name": "@varlet/axle",
  "version": "0.2.13",
  "description": "Progressive request tool based on axios",
  "type": "module",
  "main": "es/index.js",
  "module": "es/index.js",
  "types": "index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./es/*"
      ]
    }
  },
  "exports": {
    ".": "./es/index.js",
    "./use": "./es/use.js",
    "./*": "./*"
  },
  "files": [
    "es"
  ],
  "keywords": [
    "axle",
    "axios",
    "helpers",
    "vue3",
    "composition api"
  ],
  "author": "haoziqaq <357229046@qq.com>",
  "license": "MIT",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "simple-git-hooks",
    "bootstrap": "pnpm i && pnpm build && pnpm --filter ./playground i",
    "dev": "tsup src/index.ts src/use.ts --format esm --out-dir=es --watch --dts",
    "build": "tsup src/index.ts src/use.ts --format esm --out-dir=es --dts --clean",
    "dev:play": "pnpm --dir ./playground dev",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "release": "pnpm build && vr release"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec nano-staged --allow-empty --concurrent false",
    "commit-msg": "pnpm exec commitlint -e $1"
  },
  "nano-staged": {
    "*.{vue,ts,tsx,js,jsx,less,css}": "pnpm format",
    "*.{vue,ts,tsx,js,jsx}": "pnpm lint"
  },
  "dependencies": {
    "@varlet/shared": "^2.19.2",
    "axios": "^1.4.0",
    "minimatch": "^9.0.3",
    "qs": "^6.11.0"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "tsup": "7.2.0",
    "nano-staged": "0.8.0",
    "rimraf": "^5.0.1",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^5.1.5",
    "vue": "3.3.4",
    "eslint": "^8.43.0",
    "prettier": "^2.8.8",
    "@varlet/release": "0.0.3",
    "@varlet/eslint-config": "^2.19.2",
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@types/minimatch": "^5.1.2",
    "@types/qs": "^6.9.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/varletjs/axle.git"
  },
  "bugs": {
    "url": "https://github.com/varletjs/axle/issues"
  },
  "packageManager": "pnpm@8.7.5",
  "engines": {
    "pnpm": ">=8.0"
  }
}